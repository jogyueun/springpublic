{{>layouts/header}}
<table class="table">
    <a href="/signs/new">New Article</a>
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#signList}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/signs/{{id}}">{{email}}</a></td>
            <td>{{password}}</td>
        </tr>
    {{/signList}}
    </tbody>
</table>
{{>layouts/footer}}